<script
    async
    src="<%= umami.src %>"
    data-do-not-track="true"
    data-website-id="<%= umami.websiteId %>"
    <% if (umami.hostUrl) { %>
        data-host-url="<%= umami.hostUrl %>"
    <% } %>
    <% if (umami.domains) { %>
        data-domains="<%= umami.domains %>"
    <% } %>
></script>
